{
  "name": "@aaronmeese.com/gui",
  "version": "1.0.0",
  "description": "aaronmeese.com GUI",
  "module": "dist/esm.js",
  "main": "dist/main.js",
  "style": "dist/main.css",
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "eslint": "eslint index.js 'src/**/*.js'",
    "build": "webpack",
    "build:esm": "rollup -c",
    "watch": "webpack --watch",
    "watch:esm": "rollup -c --watch",
    "stylelint": "stylelint index.scss src/**/*.scss",
    "prepublishOnly": "npm run test && rm ./dist/* && NODE_ENV=production npm run build && npm run build:esm"
  },
  "files": [
    "dist/esm.js",
    "dist/esm.js.map",
    "dist/main.js",
    "dist/main.js.map",
    "dist/main.css",
    "dist/main.css.map",
    "README.md"
  ],
  "dependencies": {
    "hyperapp": "^1.2.10",
    "hyperapp-nestable": "^1.0.0"
  },
  "devDependencies": {
		"@aaronmeese.com/eslint-config": "workspace:*",
		"@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@aaronmeese.com/stylelint-config": "workspace:*",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "file-loader": "^5.1.0",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.32.13",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",

    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@aaronmeese.com/eslint-config"
  },
  "stylelint": {
    "extends": "@aaronmeese.com/stylelint-config"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./doc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "brand": {
            "title": "aaronmeese.com GUI API",
            "description": "aaronmeese.com GUI API Documentation"
          },
          "lint": {
            "enable": false
          },
          "coverage": {
            "enable": false
          },
          "undocumentIdentifier": {
            "enable": false
          }
        }
      },
      {
        "name": "esdoc-publish-html-plugin"
      }
    ]
  }
}
