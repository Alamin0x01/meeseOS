{
  "name": "@meeseOS/google-api-provider",
  "version": "2.0.0",
  "description": "Google API Service Provider for meeseOS",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "test": "npm run eslint",
    "eslint": "eslint *.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && rm ./dist/* && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@meeseOS/eslint-config": "workspace:*",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.18.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "extends": "@meeseOS/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
