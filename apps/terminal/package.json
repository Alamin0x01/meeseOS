{
  "name": "@aaronmeese.com/terminal",
  "version": "2.3.16",
  "description": "aaronmeese.com Terminal Application",
  "scripts": {
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss --fix",
    "test": "npm run eslint && npm run stylelint",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && npm run stylelint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "files": [
    "dist/",
    "metadata.json",
    "server.js"
  ],
  "devDependencies": {
    "@aaronmeese.com/eslint-config": "workspace:*",
    "@aaronmeese.com/stylelint-config": "workspace:*",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.18.0",
    "mini-css-extract-plugin": "^2.5.3",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "clipboard-polyfill": "^2.8.6",
    "dotenv": "^16.0.0",
    "hyperapp": "^1.2.10",
    "node-pty": "^0.9.0",
    "uuid": "^7.0.1",
    "xterm": "^4.4.0",
    "xterm-addon-attach": "^0.5.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "meeseOS": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@aaronmeese.com/eslint-config"
  },
  "stylelint": {
    "extends": ["stylelint-config-standard-scss", "@aaronmeese.com/stylelint-config"]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
