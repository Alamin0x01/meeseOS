{
  "name": "@aaronmeese.com/terminal",
  "version": "1.0.0",
  "description": "aaronmeese.com Terminal Application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run eslint && npm run stylelint && rm ./dist/* && NODE_ENV=production npm run build"
  },
  "files": [
    "dist/",
    "metadata.json",
    "server.js"
  ],
  "dependencies": {
    "clipboard-polyfill": "^2.8.6",
    "hyperapp": "^1.2.10",
    "node-pty": "^0.9.0",
    "uuid": "^7.0.1",
    "xterm": "^4.4.0",
    "xterm-addon-attach": "^0.5.0",
    "xterm-addon-fit": "^0.3.0"
  },
  "osjs": {
    "type": "package"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@osjs/eslint-config": "^2.0.0",
    "@osjs/stylelint-config": "^2.0.0",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "file-loader": "^5.1.0",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.32.13",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {}
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
