{
  "name": "@meeseOS/games",
  "version": "1.1.0",
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "build": "webpack",
    "watch": "webpack --watch",
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss --fix",
    "prepublishOnly": "npm run test && rm ./dist/* && npm run build"
  },
  "files": [
    "dist/",
    "metadata.json"
  ],
  "meeseOS": {
    "type": "package"
  },
  "devDependencies": {
    "@meeseOS/eslint-config": "workspace:*",
    "@meeseOS/stylelint-config": "workspace:*",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.18.0",
    "mini-css-extract-plugin": "^2.5.3",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "js-dos": "^7.4.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@meeseOS/eslint-config"
  },
  "stylelint": {
    "extends": ["stylelint-config-standard-scss", "@meeseOS/stylelint-config"]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
