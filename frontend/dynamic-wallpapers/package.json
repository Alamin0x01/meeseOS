{
  "name": "@meeseOS/dynamic-wallpapers",
  "version": "1.2.3",
  "description": "meeseOS Dynamic Wallpapers",
  "files": [
    "dist/",
    "metadata.json"
  ],
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "eslint": "eslint index.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "stylelint": "stylelint index.scss src/**/*.scss --fix",
    "prepublishOnly": "npm run test && rm -rf ./dist/* && NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "@meeseOS/eslint-config": "workspace:*",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.18.0",
    "mini-css-extract-plugin": "^2.5.3",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@meeseOS/common": "workspace:*",
    "hexells": "^1.1.7",
    "vanta": "^0.5.24",
    "three": "^0.145.0"
  },
  "meeseOS": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "extends": "@meeseOS/eslint-config"
  },
  "stylelint": {
    "extends": ["stylelint-config-standard-scss", "@meeseOS/stylelint-config"]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
